{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ThreadPoolSettings": {
    "MinThreads": 8,
    "MinCompletionPortThreads": 8,
    "MaxThreads": 32767,
    "MaxCompletionPortThreads": 1000
  },
  "Audience": {
    "Issuer": "gala",
    "Audience": "galaServices",
    "Expiration": 3600
  },
  "Consul": {
    "ConsulAddress": "http://consul_client:8500",
    "Address": "library_gateway",
    "HealthRoute": "health",
    "Name": "library_services_gateway"
  }
}
